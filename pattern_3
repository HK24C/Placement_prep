#include<stdio.h>

int main()
{
    int i,j,n,k;
    printf("Enter the number of rows and columns: ");
    scanf("%d",&n);

    for(i=1;i<=n;i++){
        for(j=1;j<=n;j++){
        if(i==1||i==n||j==1||j==n)
            printf("* ");
        else{
            printf("%c ",k+65);
            k++;
        }    
    }
    printf("\n");
}
return 0;
}


    
