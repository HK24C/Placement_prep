#include<stdio.h>
#include<string.h>

int main()
{
    int i,l,flag=0;
    char str[20];
    printf("Enter the string: ");
    gets(str);
    l=strlen(str);

    for(i=0;i<l/2;i++){
        if(str[i]==str[l-i-1])
        flag++;
    }
    if(flag==i)
        printf("String is palindrome");
    else
        printf("String is not palindrome");

    return 0;
}


